{

    "manifest_version": 3,
    "name": "CopyNoCode",
    "version": "1.1",

    "description": "",

    "icons": {
        "48": "icon.png"
    },

    "permissions": [
        "activeTab",
        "storage"
    ],

    "content_scripts": [
        {
            "matches": ["*://*.bubble.io/page*"],
            "js": ["jquery.min.js", "main.js"],
            "css": ["style.css"],
            "resources": ["icon.png"]
        }
    ]
}
